{
  "name": "quickchart",
  "version": "2.0.0",
  "type": "module",
  "main": "dist/index.js",
  "license": "AGPL-3.0",
  "homepage": "https://quickchart.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/typpo/quickchart.git"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node --max-http-header-size=65536 dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ tests/",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "dependencies": {
    "@viz-js/viz": "^3.11.0",
    "canvas": "^3.1.0",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-node-canvas": "^5.0.0",
    "chartjs-plugin-annotation": "^3.0.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^8.0.0",
    "patternomaly": "^1.3.2",
    "pdfkit": "^0.15.0",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.0",
    "qrcode": "^1.5.4",
    "qs": "^6.14.0",
    "sharp": "^0.33.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "@types/pdfkit": "^0.17.5",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.16.0",
    "jiti": "^2.6.1",
    "prettier": "^3.4.0",
    "supertest": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.17.0",
    "vitest": "^3.0.0"
  }
}
